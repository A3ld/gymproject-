{
    "java.project.sourcePaths": ["src"],
    "java.project.outputPath": "target/classes",
    "java.project.referencedLibraries": {
        "include": [
            "lib/**/*.jar",
            "target/dependency/**/*.jar"
        ],
        "exclude": []
    },
    "java.configuration.updateBuildConfiguration": "automatic",
    "java.dependency.packagePresentation": "hierarchical",
    "java.import.gradle.enabled": false,
    "java.import.maven.enabled": true,
    "editor.tokenColorCustomizations": {
        "textMateRules": [
            {
                "scope": "storage.modifier.import.java",
                "settings": {
                    "foreground": "#C586C0"
                }
            },
            {
                "scope": "storage.type.java",
                "settings": {
                    "foreground": "#4EC9B0"
                }
            },
            {
                "scope": "entity.name.type.class.java",
                "settings": {
                    "foreground": "#4EC9B0"
                }
            },
            {
                "scope": "variable.other.object.java",
                "settings": {
                    "foreground": "#9CDCFE"
                }
            },
            {
                "scope": "entity.name.function.java",
                "settings": {
                    "foreground": "#DCDCAA"
                }
            },
            {
                "scope": "string.quoted.double.java",
                "settings": {
                    "foreground": "#CE9178"
                }
            }
        ]
    },
    "java.jdt.ls.vmargs": "-XX:+UseParallelGC -XX:GCTimeRatio=4 -XX:AdaptiveSizePolicyWeight=90 -Dsun.zip.disableMemoryMapping=true -Xmx2G -Xms100m",
    "terminal.integrated.env.windows": {
        "PATH_TO_FX": "C:\\Users\\abdul\\.m2\\repository\\org\\openjfx\\javafx-controls\\21.0.3"
    }
}
